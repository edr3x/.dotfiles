{
    "name": "custom-snippets",
    "author": "edr3x",
    "engines": {
        "vscode": "^1.11.0"
    },
    "contributes": {
        "snippets": [
            {
                "language": "dart",
                "path": "./dart.json"
            },
            {
                "language": "lua",
                "path": "./lua.json"
            },
            {
                "language": [
                    "shellscript",
                    "shell",
                    "sh"
                ],
                "path": "./shell.json"
            },
            {
                "language": "typescript",
                "path": "./typescript.json"
            },
            {
                "language": "rust",
                "path": "./rust.json"
            }
        ]
    }
}
